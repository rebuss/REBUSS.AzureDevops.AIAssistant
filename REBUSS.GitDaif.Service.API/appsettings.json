{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "GitDaif.Service": {
    "PersonalAccessToken": "yourPersonalAccessToken",
    "Organization": "yourOrganizationName",
    "ProjectName": "yourProjectName",
